{
  "name": "artagent-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for the Art Agent application",
  "scripts": {
    "dev": "pnpm run --parallel dev",
    "dev:web": "pnpm run --filter web dev",
    "dev:server": "pnpm run --filter server dev",
    "build": "pnpm run --recursive build",
    "build:web": "pnpm run --filter web build",
    "build:server": "pnpm run --filter server build",
    "clean": "pnpm run --recursive clean",
    "install:all": "pnpm install"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  }
}